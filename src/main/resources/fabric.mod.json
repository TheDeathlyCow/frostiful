    "mixins": [
        "frostiful.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.15.7",
        "fabric-api": ">=0.91.0",
        "minecraft": "~1.20.1",
        "thermoo": ">=2.2",
        "mm": ">=2.3",
        "java": ">=17"
    },
    "breaks": {
        "healthoverlay": "<7.2.1",
        "colorfulhearts": "<4.0.1",
        "thermoo": "<2.0.0"
    },
    "suggests": {
        "scorchful": "*"
    },
    "accessWidener": "frostiful.accesswidener",
    "custom": {
        "cardinal-components": [
            "frostiful:living_entity",
            "frostiful:brushable"
        ]
